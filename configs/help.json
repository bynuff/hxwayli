    {
      "header": "  Command description",
      "text": [
        "   pack <project_path> : path to dev lib",
        "                   ***",
        "   mkenv <env_name>    : create new environment",
        "   env <env_name>      : set active environment from existing",
        "   clenv <env_name>    : remove environment",
        "   lsenv               : print list of existing environments",
        "                   ***",
        "   uninst              : remove wayli tool",
        "   help                : show tool information"
      ]
    }
  ]
